        {
            default xml namespace = SpreadsheetNamespaces.ans;
            
            var entriesInfo:XMLList = rawFeed.entry;
            
            for each (var entryInfo:XML in entriesInfo)
            {
                var entry:RowEntry = new RowEntry(entryInfo);
                _entries.push(entry);
            }
